<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/admin_dashboard_bg"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/back_arrow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:src="@drawable/ic_arrow_back"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.4" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="255dp"
        android:layout_height="109dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/circle_shape"
        android:backgroundTint="#445036"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        tools:ignore="MissingConstraints">

        <TextView
            android:layout_width="246dp"
            android:layout_height="47dp"
            android:fontFamily="@font/courgette"
            android:padding="5dp"
            android:text="  Appointments"
            android:textColor="#FFF"
            android:textSize="22sp"
            android:textStyle="bold" />

        <Button
            android:id="@+id/appointmentsBtn"
            android:layout_width="108dp"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginTop="5dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/circle_shape"
            android:backgroundTint="#C48F40"
            android:text="-->"
            android:textColor="#FFFFFF"
            android:textSize="25sp"
            android:textStyle="bold" />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="254dp"
        android:layout_height="110dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/circle_shape"
        android:backgroundTint="#445036"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView17"
        tools:ignore="MissingConstraints">

        <TextView
            android:layout_width="232dp"
            android:layout_height="49dp"
            android:fontFamily="@font/courgette"
            android:padding="5dp"
            android:text="  Calendar"
            android:textColor="#FFF"
            android:textSize="25sp"
            android:textStyle="bold" />

        <Button
            android:id="@+id/calendarBtn"
            android:layout_width="109dp"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginTop="5dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/circle_shape"
            android:backgroundTint="#C48F40"
            android:fontFamily="@font/courgette"
            android:text="Create \n New"
            android:textAllCaps="false"
            android:textColor="#FFFFFF"
            android:textSize="18sp"
            android:textStyle="bold" />

        <Button
            android:id="@+id/currentCalBtn"
            android:layout_width="109dp"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="-50dp"
            android:background="@drawable/circle_shape"
            android:backgroundTint="#C48F40"
            android:fontFamily="@font/courgette"
            android:text="Current Times"
            android:textAllCaps="false"
            android:textColor="#FFFFFF"
            android:textSize="18sp"
            android:textStyle="bold" />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="258dp"
        android:layout_height="108dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/circle_shape"
        android:backgroundTint="#445036"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout4"
        tools:ignore="MissingConstraints">

        <TextView
            android:layout_width="219dp"
            android:layout_height="47dp"
            android:fontFamily="@font/courgette"
            android:padding="5dp"
            android:text="  Profile"
            android:textColor="#FFF"
            android:textSize="25sp"
            android:textStyle="bold" />

        <Button
            android:id="@+id/profileBtn"
            android:layout_width="108dp"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginTop="5dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/circle_shape"
            android:backgroundTint="#C48F40"
            android:text="-->"
            android:textColor="#FFFFFF"
            android:textSize="25sp"
            android:textStyle="bold" />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="253dp"
        android:layout_height="109dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/circle_shape"
        android:backgroundTint="#445036"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        tools:ignore="MissingConstraints">

        <TextView
            android:layout_width="257dp"
            android:layout_height="48dp"
            android:fontFamily="@font/courgette"
            android:padding="5dp"
            android:text="  Customers"
            android:textColor="#FFF"
            android:textSize="25sp"
            android:textStyle="bold" />

        <Button
            android:id="@+id/customersBtn"
            android:layout_width="108dp"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginTop="5dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/circle_shape"
            android:backgroundTint="#C48F40"
            android:text="-->"
            android:textColor="#FFFFFF"
            android:textSize="25sp"
            android:textStyle="bold" />


    </LinearLayout>

    <TextView
        android:id="@+id/textView17"
        android:layout_width="308dp"
        android:layout_height="69dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="Admin Dashboard"
        android:textColor="#C48F40"
        android:textSize="32sp"
        android:textStyle="bold"
        app:fontFamily="@font/courgette"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>